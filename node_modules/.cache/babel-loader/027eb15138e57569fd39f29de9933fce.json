{"remainingRequest":"/Users/wilson/Desktop/my-app/vue-app/node_modules/babel-loader/lib/index.js!/Users/wilson/Desktop/my-app/vue-app/src/router/index.js","dependencies":[{"path":"/Users/wilson/Desktop/my-app/vue-app/src/router/index.js","mtime":1610887914142},{"path":"/Users/wilson/Desktop/my-app/vue-app/node_modules/cache-loader/dist/cjs.js","mtime":499162500000},{"path":"/Users/wilson/Desktop/my-app/vue-app/node_modules/babel-loader/lib/index.js","mtime":315532800000}],"contextDependencies":[],"result":[{"type":"Buffer","data":"base64:"},{"version":3,"sources":["/Users/wilson/Desktop/my-app/vue-app/src/router/index.js"],"names":["Vue","VueRouter","Login","Home","use","routes","path","redirect","component","router","beforeEach","to","from","next","tokenStr","window","sessionStorage","getItem"],"mappings":"AAAA,OAAOA,GAAP,MAAgB,KAAhB;AACA,OAAOC,SAAP,MAAsB,YAAtB;AACA,OAAOC,KAAP,MAAkB,yBAAlB;AACA,OAAOC,IAAP,MAAiB,wBAAjB;AAGAH,GAAG,CAACI,GAAJ,CAAQH,SAAR;AAEA,IAAMI,MAAM,GAAG,CACb;AAAEC,EAAAA,IAAI,EAAE,GAAR;AAAaC,EAAAA,QAAQ,EAAE;AAAvB,CADa,EAEb;AAAED,EAAAA,IAAI,EAAE,QAAR;AAAkBE,EAAAA,SAAS,EAAEN;AAA7B,CAFa,EAGb;AAAEI,EAAAA,IAAI,EAAE,OAAR;AAAiBE,EAAAA,SAAS,EAAEL;AAA5B,CAHa,CAAf;AAMA,IAAMM,MAAM,GAAG,IAAIR,SAAJ,CAAc;AAC3BI,EAAAA,MAAM,EAANA;AAD2B,CAAd,CAAf,C,CAIA;;AACAI,MAAM,CAACC,UAAP,CAAkB,UAACC,EAAD,EAAKC,IAAL,EAAWC,IAAX,EAAoB;AACpC;AACA;AACA;AACA;AACA,MAAIF,EAAE,CAACL,IAAH,KAAY,QAAhB,EAA0B,OAAOO,IAAI,EAAX,CALU,CAMpC;;AACA,MAAMC,QAAQ,GAAGC,MAAM,CAACC,cAAP,CAAsBC,OAAtB,CAA8B,OAA9B,CAAjB,CAPoC,CAQpC;;AACA,MAAI,CAACH,QAAL,EAAe,OAAOD,IAAI,CAAC,QAAD,CAAX;AACfA,EAAAA,IAAI;AACL,CAXD;AAaA,eAAeJ,MAAf","sourcesContent":["import Vue from 'vue'\nimport VueRouter from 'vue-router'\nimport Login from '../components/Login.vue'\nimport Home from '../components/Home.vue'\n\n\nVue.use(VueRouter)\n\nconst routes = [\n  { path: '/', redirect: '/login' },\n  { path: '/login', component: Login },\n  { path: '/home', component: Home },\n]\n\nconst router = new VueRouter({\n  routes\n})\n\n// 挂载路由导航守卫\nrouter.beforeEach((to, from, next) => {\n  // to 将要访问\n  // from 代表从哪个路径跳转而来\n  // next 是一个函数， 表示放行\n  // next() 放行  next('/login') 强制跳转\n  if (to.path === '/login') return next() \n  // 获取token\n  const tokenStr = window.sessionStorage.getItem('token')\n  // 如果token不存在 强制跳转登录页\n  if (!tokenStr) return next('/login')\n  next()\n} )\n\nexport default router\n"]}]}